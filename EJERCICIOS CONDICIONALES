Ejercicio 1: Escribe un programa que solicite al usuario su edad y muestre un mensaje indicando si es mayor de edad o no.​ let edad= prompt("¿Cual es tu edad?")

if (edad >= 18) {
    alert("Eres mayor de edad");
}

else {
    alert ("Eres menor de edad")
}


let numeroUsuario = parseInt(prompt("Ingrese un numero del 1 al 7"));

if (numeroUsuario === 1) {
    alert("Hoy es lunes")
}

if (numeroUsuario === 2) {
    alert("Hoy es Martes")
}

if (numeroUsuario === 3) {
    alert("Hoy es Miercoles")
}

if (numeroUsuario === 4) {
    alert("Hoy es Jueves")
}

if (numeroUsuario === 5) {
    alert("Hoy es Viernes")
}

if (numeroUsuario === 6) {
    alert("Hoy es Sabado")
}

if (numeroUsuario === 7) {
    alert("Hoy es Domingo")
}

else{
    alert("ERROR")
} 


